{
  "root": true,
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/vue3-essential",
    "@vue/eslint-config-typescript/recommended",
    "@vue/eslint-config-prettier",
    "plugin:quasar/standard"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    // Defina suas regras personalizadas aqui
    "vue/multi-word-component-names": "off" // Exemplo: Desabilitar a regra para nomes de componentes com m√∫ltiplas palavras
  }
}
